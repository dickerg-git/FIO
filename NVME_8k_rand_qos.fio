[global]
filename=/dev/nvme0n1
direct=1
refill_buffers
norandommap
randrepeat=0
ioengine=libaio  #was posixaio
loops=1
group_reporting
size=50%

[8k_RND_WR_test]
stonewall
name=8k_RND_WR_test
bs=8k
iodepth=32
numjobs=2
rw=randwrite
rwmixread=0

[8k_RND_RD_test]
stonewall
name=8k_RND_RD_test
bs=8k
iodepth=32
numjobs=2
rw=randread
rwmixread=100


[8k_RND_RW_test]
stonewall
name=8k_RND_RW_test
bs=8k
iodepth=32
numjobs=2
rw=randrw
rwmixread=30
